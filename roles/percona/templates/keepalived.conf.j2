vrrp_instance {{ cluster_name }} {
    state {{ node_role }}
    interface {{ interface_name }}
    virtual_router_id 51
    priority {{ node_priority }}
    advert_int 1
    authentication {
        auth_type PASS
        auth_pass {{ keepalived_auth_pass }}
    }
    virtual_ipaddress {
        {{ floating_ip }}
    }
}
